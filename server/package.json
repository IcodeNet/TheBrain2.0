{
  "name": "thebrain-server",
  "version": "0.0.3",
  "description": "Server for TheBrain 2.0",
  "main": "src/startup.js",
  "scripts": {
    "test": "BABEL_ENV=serverTest NODE_ENV=TESTING NODE_PATH='src' ./node_modules/.bin/jest src/**/*.spec.js --coverage --coverageDirectory artifacts ",
    "dev": "BABEL_ENV=production NODE_PATH='src' nodemon --inspect=6666 src/startup.js --watch src --exec babel-node",
    "startProdDb": "NODE_ENV='production' NODE_PATH='src' nodemon --debug=6666 src/startup.js --watch src --exec babel-node",
    "start": "BABEL_ENV=production NODE_PATH='src' babel-node src/startup.js",
    "startLocalDB": "docker stop mongo_instance_001 || true && docker rm mongo_instance_001 || true && docker run -p 27017:27017 --name mongo_instance_001 -d mongo",
    "loadTestDBData": "mongo localhost:27017/thebrain < ../dbImporter/improvedDb.js",
    "startDockerizedMongo": "docker run -d -p 27017:27017 mongo",
    "flow": "flow",
    "standard": "standard --verbose --fix | snazzy",
    "deploySetup": "./.scripts/deploySetup.sh",
    "deploy": "./.scripts/deploy.sh",
    "heroku-postbuild": "npm install",
    "build": "BABEL_ENV=development NODE_ENV=production babel src --out-dir lib --watch",
    "build:lambda": "BABEL_ENV=lambda NODE_ENV=production babel src --out-dir libLambda",
    "build:lambda:watch": "npm run build:lambda -- --watch",
    "start:lambda": "NODE_PATH='libLambda' node libLambda/startup-serverless.js",
    "deployToServerless": "serverless deploy -v"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.1",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "graphql": "^0.11.7",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^0.10.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongodb": "^2.2.24",
    "node-fetch": "^1.6.3",
    "node-schedule": "^1.2.4",
    "nodemailer": "^4.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "serverless-http": "^1.5.3",
    "serverless-offline": "^3.16.0",
    "serverless-stack-output": "^0.2.3",
    "urlencode": "^1.1.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "flow-typed/"
    ],
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "beforeAll",
      "after",
      "afterEach",
      "afterAll",
      "it",
      "expect",
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgandecki/TheBrain2.0.git"
  },
  "devDependencies": {
    "apollo-client": "^1.9.3",
    "apollo-test-utils-with-context": "^0.3.5",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-rewrite-with-root": "^0.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-babel6-aws-lambda": "^1.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "casual": "^1.5.12",
    "codecov": "^2.1.0",
    "flow-bin": "^0.40.0",
    "jest": "^22.0.4",
    "module-resolver": "^1.0.0",
    "nodemon": "^1.11.0",
    "serverless-offline": "^3.16.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "testdouble": "^2.1.2",
    "thenify": "^3.3.0",
    "tingodb-promise": "0.0.2"
  }
}
