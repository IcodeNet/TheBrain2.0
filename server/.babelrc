{
  "plugins": [
  ],
  "presets": [
    "env",
    "stage-2",
    "flow"
  ],
  "env": {
    "lambda": {
      "presets": [
        ["env", {
          "targets": {
            "node": "6.1.0"
          }
        }]
      ]
    },
    "test": {
      "plugins": [
        [
          "module-rewrite-with-root",
          {
            "replaceFunc": "./src/testHelpers/replace-for-testing.js",
            "optionalRoot": "server/"
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        [
          "module-rewrite-with-root",
          {
            "replaceFunc": "./src/testHelpers/replace-for-testing.js",
            "optionalRoot": "server/"
          }
        ]
      ]
    },
    "serverTest": {
      "plugins": [
        [
          "module-rewrite-with-root",
          {
            "replaceFunc": "./src/testHelpers/replace-for-testing.js",
          }
        ]
      ]
    },
    "cypressTest": {
      "presets": [
        "es2015",
        "stage-2",
        "flow"
      ],
      "plugins": [
        [
          "module-rewrite-with-root",
          {
            "replaceFunc": "./server/src/testHelpers/replace-for-testing.js"
          }
        ]
      ]
    }
  }
}